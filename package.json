{
  "name": "@ameen-nexus/validator",
  "version": "0.1.0",
  "private": false,
  "description": "Laravel-like validation library for React (TypeScript)",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "require": "./dist/react/index.cjs.js",
      "import": "./dist/react/index.esm.js",
      "types": "./dist/react/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "test": "vitest",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm run test && npm run build"
  },
  "peerDependencies": {
    "axios": "^1.0.0",
    "react": ">=17"
  },
  "devDependencies": {
    "tsup": "^6.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "eslint": "^8.0.0",
    "rimraf": "^3.0.0"
  },
  "license": "MIT"
}
