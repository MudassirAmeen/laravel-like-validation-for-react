{
  "name": "@ameen-nexus/validator",
  "version": "1.0.0",
  "private": false,
  "description": "Laravel-like validation library for React (TypeScript)",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react-extensions/index.mjs",
      "require": "./dist/react-extensions/index.js",
      "types": "./dist/react-extensions/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "test": "vitest",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm run test && npm run build"
  },
  "peerDependencies": {
    "axios": "^1.0.0",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "axios": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "eslint": "^8.0.0",
    "rimraf": "^3.0.0",
    "tsup": "^6.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0"
  },
  "dependencies": {
    "axios": "^1.11.0"
  },
  "license": "MIT"
}